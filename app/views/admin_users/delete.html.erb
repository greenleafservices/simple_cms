<% @page_title = "Delete Admin" %>

<%= link_to("<< Back to List", admin_users_path, :class => 'back-link') %>

<div class="admins delete">
  <h2>Delete Admin</h2>

  <%= form_for(@adminUser, :method => 'delete') do |f| %>

    <p>Are you sure you want to permanently delete this admin?</p>
    <p class="reference-name"><%= f.object.name %></p>

    <div class="form-buttons">
      <%= f.submit("Delete Admin") %>
    </div>

  <% end %>
</div>
